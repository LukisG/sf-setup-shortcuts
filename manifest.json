{
  "manifest_version": 3,
  "name": "Sf Setup Shortcuts",
  "description": "Faster navigation through swamps of salesforce setup",
  "version": "1.0",
  "action": {
    "default_popup": "./src/sfshort.html",
    "default_icon": "sfshort.png"
  },
  "options_ui": {
    "page": "./src/options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "./src/setup.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage"
  ]
}
